{
  "name": "start",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "grant-admin:dev": "docker compose -f docker-compose.dev.yml --env-file .env.dev exec api pnpm tsx src/scripts/grantAdmin.ts",
    "grant-admin:prod": "docker compose -f docker-compose.prod.yml --env-file .env.prod exec api pnpm tsx src/scripts/grantAdmin.ts",
    "docker:up": "docker compose -f docker-compose.dev.yml --env-file .env.dev up",
    "docker:up:prod": "docker compose -f docker-compose.prod.yml --env-file .env.prod up",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:down:prod": "docker compose -f docker-compose.prod.yml down",
    "docker:build": "docker compose -f docker-compose.dev.yml --env-file .env.dev build",
    "docker:build:prod": "docker compose -f docker-compose.prod.yml --env-file .env.prod build",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:logs:prod": "docker compose -f docker-compose.prod.yml logs -f",
    "prisma:migrate": "docker compose -f docker-compose.dev.yml --env-file .env.dev exec api pnpm prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.13.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@eslint/json": "^0.13.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
